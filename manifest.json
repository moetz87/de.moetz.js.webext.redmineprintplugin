{
    "manifest_version": 2,
    "name": "WebExtension Print Example",
    "version": "1.0.0",
    "description": "A WebExtension to demonstrate how to print content from WebExtensions.",
    "author": "Marco Oetz",
    "applications": {
        "gecko": {
            "id": "dev@marcooetz.de"
        }
    },
    "icons": {
        "48": "icons/logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-scripts/printbuttons.js"
            ]
        }
    ],
    "permissions": [
        "<all_urls>"
    ]
}